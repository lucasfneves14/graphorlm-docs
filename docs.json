{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "GraphorLM Docs",
  "colors": {
    "primary": "#cc6ca7",
    "light": "#cc6ca7",
    "dark": "#6e2f84"
  },
  "appearance": {
    "default": "system",
    "strict": true
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/data-ingestion",
              "guides/chunking",
              "guides/retrieval",
              "guides/llm-integration",
              "guides/evaluation",
              "guides/api-tokens",
              "guides/integrate-workflow"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Sources",
            "pages": [
              "api-reference/sources/overview",
              "api-reference/sources/upload",
              "api-reference/sources/process",
              "api-reference/sources/list",
              "api-reference/sources/delete"
            ]
          },
          {
            "group": "Flows",
            "pages": [
              "api-reference/flows/overview",
              "api-reference/flows/list",
              "api-reference/flows/run",
              "api-reference/flows/deploy",
              {
                "group": "Datasets",
                "pages": [
                  "api-reference/flows/datasets/overview",
                  "api-reference/flows/datasets/list",
                  "api-reference/flows/datasets/update"
                ]
              }
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Product",
          "href": "https://app.graphorlm.com",
          "icon": "earth-americas"
        },
        {
          "anchor": "LinkedIn",
          "href": "https://www.linkedin.com/company/graphorlm",
          "icon": "linkedin"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:lucas@graphorlm.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Login",
      "href": "https://app.graphorlm.com/login"
    }
  },
  "footer": {
    "socials": {
      "linkedin": "https://linkedin.com/company/graphorlm"
    }
  }
}